<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CadastraCliente</servlet-name>
        <servlet-class>Servlets.CadastraCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TelaCadastro</servlet-name>
        <servlet-class>Servlets.TelaCadastro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastraEmpresa</servlet-name>
        <servlet-class>Servlets.CadastraEmpresa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Disconect</servlet-name>
        <servlet-class>Servlets.Disconect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltConta</servlet-name>
        <servlet-class>Servlets.AltConta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltEmp</servlet-name>
        <servlet-class>Servlets.AltEmp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelUser</servlet-name>
        <servlet-class>Servlets.DelUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelEmp</servlet-name>
        <servlet-class>Servlets.DelEmp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Servlets.Index</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastraCliente</servlet-name>
        <url-pattern>/CadastraCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TelaCadastro</servlet-name>
        <url-pattern>/TelaCadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastraEmpresa</servlet-name>
        <url-pattern>/CadastraEmpresa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Disconect</servlet-name>
        <url-pattern>/Disconect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltConta</servlet-name>
        <url-pattern>/AltConta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltEmp</servlet-name>
        <url-pattern>/AltEmp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelUser</servlet-name>
        <url-pattern>/DelUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelEmp</servlet-name>
        <url-pattern>/DelEmp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Index</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
